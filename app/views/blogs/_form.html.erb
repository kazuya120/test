<%= render 'layouts/errors', obj: blog %>
<%= form_with model:blog, local:true do |f| %>
<div class="form-group">
  <%= f.label :title %>
  <%= f.text_field :title, class: 'form-control blog_title' %>
</div>
<div class="form-group">
  <%= f.label :body %>
  <%= f.text_field :body, class: 'form-control blog_body' %>
</div>
<div class="form-group">
  <%= f.attachment_field :image %>
</div>
<div class="form-group">
  <%= f.submit class: 'btn btn-success' %>
</div>
<% end %>